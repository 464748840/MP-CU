<ui-sys doc title="图片">
    <view class="ui-container">
        <!-- 简介 -->
        <ui-title title="简介" depth="2" isIcon/>
        <view class="paragraph">图片</view>

        <ui-img preview ui="radius border" src="{{list[0]}}" urls="{{list}}" current="0"/>

        <!--参数-->
        <ui-parameter data="{{parameter}}"/>

        <!--壁纸-->
        <ui-menu ui="radius border mt-4">
            <ui-menu-item icon="cicon-pic" title="壁纸" arrow bindtap="toDemoWallpaper"/>
        </ui-menu>
    </view>
</ui-sys>
